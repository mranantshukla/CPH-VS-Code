{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Extension",
            "program": "${workspaceFolder}/src/frontend/extension.js",
            "cwd": "${workspaceFolder}",
            "outFiles": ["${workspaceFolder}/out/**/*.js"],
            "preLaunchTask": "npm: compile"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Extension",
            "port": 9229
        }
    ]
}

